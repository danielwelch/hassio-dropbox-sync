{
  "name": "Dropbox Sync",
  "version": "dev",
  "slug": "dropbox_sync",
  "description": "Upload your Hass.io backups to Dropbox",
  "url": "https://github.com/danielwelch/hassio-dropbox-sync",
  "startup": "before",
  "stdin": true,
  "hassio_api": true,
  "boot": "auto",
  "map": ["backup"],
  "options": {
    "oauth_access_token": "<YOUR_ACCESS_TOKEN>",
    "output": "",
    "keep_last": ""
  },
  "schema": {
    "oauth_access_token": "str",
    "output": "str",
    "keep_last": "int(0,)"
  },
  "image": "dwelch2101/dropbox-sync-{arch}"
}
